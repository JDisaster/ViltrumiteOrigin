{
    "type": "origins:multiple",
    "name": "Flight",
    "description": "Viltrumites possess the ability to fly by creating their own leverage.",
    
    "creative_flight": {
        "type": "origins:creative_flight",
        "condition": {
            "type": "origins:and",
            "conditions": [{
                "type": "origins:resource",
                "resource": "vil:scourge_virus_timer",
                "comparison": "==",
                "compare_to": 0
            },
            {
                "type": "origins:resource",
                "resource": "vil:sound_weakness_isshrieking_resource",
                "comparison": "==",
                "compare_to": 0
            },
            {
                "type": "origins:resource",
                "resource": "vil:sound_weakness_recently_sonic_boomed",
                "comparison": "==",
                "compare_to": 0
            }]
        }
    },

    "fall_flight": {
        "type": "origins:elytra_flight",
        "render_elytra": false,
        "condition": {
            "type": "origins:and",
            "conditions": [{
                "type": "origins:resource",
                "resource": "vil:scourge_virus_timer",
                "comparison": "==",
                "compare_to": 0
            },
            {
                "type": "origins:resource",
                "resource": "vil:toggle_elytra_flight_resource",
                "comparison": "==",
                "compare_to": 0
            },
            {
                "type": "origins:resource",
                "resource": "vil:flight/flight_resources_resource",
                "comparison": ">=",
                "compare_to": 3600
            }]
        }
    },

    "fall_flight_boost": {
        "type": "origins:action_on_key_press",
        "key": {
            "id": "key.sprint",
            "continuous": true
        },
        "cooldown": 1,
        "condition": {
            "type": "origins:fall_flying"
        },
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "vil:flight/flight_resources_resource",
                "comparison": "<",
                "compare_to": 7200
            },
            "if_action": {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:submerged_in",
                    "fluid": "minecraft:water"
                },
                "if_action": {
                    "type": "origins:add_velocity",
                    "z": 0.2275,
                    "space": "local"
                },
                "else_action": {
                    "type": "origins:add_velocity",
                    "z": 0.035,
                    "space": "local"
                }
            },
            "else_action": {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:submerged_in",
                    "fluid": "minecraft:water"
                },
                "if_action": {
                    "type": "origins:add_velocity",
                    "z": 0.65,
                    "space": "local"
                },
                "else_action": {
                    "type": "origins:add_velocity",
                    "z": 0.1,
                    "space": "local"
                }
            }
        }
    },

    "initial_fall_flight_boost": {
        "type": "origins:action_on_key_press",
        "key": {
            "id": "key.sprint"
        },
        "cooldown": 20,
        "condition": {
            "type": "origins:fall_flying"
        },
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "vil:flight/flight_resources_resource",
                "comparison": "<",
                "compare_to": 7200
            },
            "if_action": {
                "type": "origins:and",
                "actions": [{
                    "type": "origins:add_velocity",
                    "z": 1.25,
                    "space": "local"
                },
                {
                    "type": "origins:play_sound",
                    "sound": "entity.ender_dragon.shoot",
                    "volume": 5,
                    "pitch": 2
                }]
            },
            "else_action": {
                "type": "origins:and",
                "actions": [{
                    "type": "origins:add_velocity",
                    "z": 2.5,
                    "space": "local"
                },
                {
                    "type": "origins:play_sound",
                    "sound": "entity.generic.explode",
                    "volume": 5,
                    "pitch": 2
                }]
            }
        }
    },

